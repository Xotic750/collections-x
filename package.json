{
    "name": "collections-x",
    "version": "1.0.1",
    "description": "ES6 collections fallback library: Map and Set.",
    "homepage": "https://github.com/Xotic750/collections-x",
    "author": {
        "name": "Graham Fairweather",
        "email": "xotic750@gmail.com"
    },
    "copyright": "Copyright (c) 2015",
    "keywords": [
        "es6",
        "Map",
        "Set",
        "library",
        "javascript",
        "nodejs",
        "browser"
    ],
    "main": "index.js",
    "engines": {
        "node": "*"
    },
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Xotic750/collections-x.git"
    },
    "bugs": {
        "url": "https://github.com/Xotic750/collections-x/issues"
    },
    "dependencies": {
        "es-abstract": "^1.4.3",
        "define-properties": "^1.1.2",
        "define-property-x": "^1.0.3",
        "is-string": "^1.0.4",
        "is-array-like-x": "^1.0.3",
        "is-surrogate-pair-x": "^1.0.2",
        "is-primitive": "^2.0.0",
        "noop-x": "^1.0.5",
        "index-of-x": "^1.0.4",
        "assert-is-callable-x": "^1.0.5",
        "assert-is-object-x": "^1.0.3",
        "big-counter-x": "^1.0.1"
    },
    "devDependencies": {
        "es5-shim": "^4.3.1",
        "jscs": "^2.5.1",
        "uglify-js": "^2.6.1",
        "browserify": "12.0.1",
        "jasmine-node": "^1.14.5",
        "jsdoc-to-markdown": "^1.2.1"
    },
    "scripts": {
        "test": "jasmine-node --matchall tests/spec/",
        "style": "jscs index.js",
        "docs": "jsdoc2md --name-format code --example-lang js table index.js > README.md",
        "build": "browserify -e index.js -o lib/collections-x.js -u 'crypto' -s returnExports",
        "build-uglify": "uglifyjs lib/collections-x.js -o lib/collections-x.min.js --compress unused=false,dead_code=false --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/collections-x.map"
    }
}
