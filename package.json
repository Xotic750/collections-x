{
  "name": "collections-x",
  "version": "1.0.13",
  "description": "ES6 collections fallback library: Map and Set.",
  "homepage": "https://github.com/Xotic750/collections-x",
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2015-2016",
  "keywords": [
    "es6",
    "Map",
    "Set",
    "library",
    "javascript",
    "nodejs",
    "browser"
  ],
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/collections-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/collections-x/issues"
  },
  "dependencies": {
    "is-callable": "^1.1.2",
    "define-properties-x": "^1.1.4",
    "is-string": "^1.0.4",
    "is-array-like-x": "^1.0.13",
    "is-surrogate-pair-x": "^1.0.12",
    "is-primitive": "^2.0.0",
    "index-of-x": "^1.0.17",
    "assert-is-callable-x": "^1.0.15",
    "assert-is-object-x": "^1.0.14",
    "big-counter-x": "^1.0.12",
    "has-symbol-support-x": "^1.0.11",
    "is-nil-x": "^1.0.10",
    "has-own-property-x": "^1.0.12"
  },
  "devDependencies": {
    "es5-shim": "^4.5.4",
    "es6-shim": "^0.34.4",
    "json3": "^3.3.2",
    "jscs": "^2.9.0",
    "uglify-js": "^2.6.1",
    "browserify": "^13.0.0",
    "browserify-derequire": "^0.9.4",
    "jasmine-node": "^1.14.5",
    "jsdoc-to-markdown": "^1.3.3",
    "nsp": "^2.2.0"
  },
  "scripts": {
    "test": "jasmine-node --matchall tests/spec/",
    "security": "nsp check",
    "style": "jscs index.js",
    "docs": "jsdoc2md --name-format --example-lang js index.js > README.md",
    "build": "browserify -p browserify-derequire -e index.js -o lib/collections-x.js -u 'crypto' -s returnExports",
    "build-uglify": "uglifyjs lib/collections-x.js -o lib/collections-x.min.js --compress --keep-fnames --mangle --beautify ascii_only=true,beautify=false --source-map lib/collections-x.map"
  }
}
